# Axom Observer Configuration Template
# Copy this file to .env and fill in your actual values

# Required: Agent Details from your Axom Portal
CUSTOMER_ID=your-agent-name
AGENT_ID=your-agent-id
CLIENT_ID=your-client-id
CLIENT_SECRET=your-client-secret

# Optional: Backend Configuration
BACKEND_URL=https://api.axom.ai/ingest

# Optional: Logging Configuration
LOG_LEVEL=info
LOG_ALL_TRAFFIC=true

# Optional: Container Configuration
MAIN_AI_CONTAINER_NAME=my-ai-app

# Optional: Dashboard Configuration (if enabled)
OBSERVER_DASHBOARD_USER=
OBSERVER_DASHBOARD_PASS=
